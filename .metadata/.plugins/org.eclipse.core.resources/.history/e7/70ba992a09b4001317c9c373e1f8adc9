#include<stdio.h>

unsigned int cont;

int main(void) {

	for (cont = 0; cont < 65535; cont++) {

		printf("%d - %c\n", cont, cont);
	}

	return 0;

}

#include <stdio.h>

int filtro(int entrada) {

	return entrada & 0b11;
}

int separa(int operando, int alto) {

	if (alto)
		return (operando & 0b10) >> 1;
	else
		return operando & 0b01;
}

int resolve(int operando, char operador) {
	if (operador == '&' || operador == '|') {
		switch (operador) {
		case '&':
			return separa(operando, 1) & separa(operando, 0);
			break;
		case '|':
			return separa(operando, 1) | separa(operando, 0);
			break;
		default:
			return -1;
		}
	} else
		return -1;
}

int main(int argc, char **argv) {

	int cont;
	char operacao;

	operacao = '&';

	printf("%d\n", argc);

	for (cont = 0; cont < argc; cont++) {
		printf("%d => %s\n", cont, argv[cont]);
	}

	for (cont = 0; cont < 4; cont++) {
		printf("%d %c %d = %d\n", separa(cont, 1), operacao, separa(cont, 0),
				resolve(cont, operacao));
	}

	return 0;
}
